@model AsistenciaComedor.Data.Entities.Nivel

@{
    ViewData["Title"] = "Eliminar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Eliminar</h2>

<h3>¿Está seguro de eliminar este registro?</h3>
<div>
    <h4>Nivel</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.nombreNivel)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.nombreNivel)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <label for="mySubmit" class="btn btn-default"><i class="glyphicon glyphicon-trash"></i> Eliminar</label>
        <input id="mySubmit" type="submit" value="Go" class="hidden" >
        <a asp-action="Index" class="btn btn-success"><i class="glyphicon glyphicon-arrow-left"></i> Regresar</a>
    </form>
</div>
